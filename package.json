{
  "private": true,
  "name": "devin-status-checker",
  "version": "1.0.0",
  "scripts": {
    "test": "pnpm exec node tests/extension.spec.ts",
    "prepare": "pnpm exec husky install",
    "build": "pnpm exec parcel build src/manifest.json --dist-dir dist --no-content-hash --no-source-maps --no-cache --detailed-report 0",
    "watch": "pnpm exec parcel watch src/manifest.json --dist-dir dist"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write"
    ]
  },
  "renovate": {
    "extends": [
        "config:recommended"
    ]
  },
  "devDependencies": {
    "@types/chrome": "^0.0.267",
    "@types/node": "^20.12.8",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "parcel": "^2.12.0",
    "playwright": "^1.43.1",
    "prettier": "^3.2.5"
  }
}
